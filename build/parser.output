Grammar

    0 $accept: s $end

    1 s: k

    2 k: declaracoes k
    3  | declaracoes
    4  | erro

    5 erro: TOKEN_ERRO k
    6     | TOKEN_ERRO

    7 declaracoes: decl_variavel
    8            | decl_vetor
    9            | decl_funcao

   10 tipo: TK_PR_INT
   11     | TK_PR_FLOAT
   12     | TK_PR_BOOL
   13     | TK_PR_CHAR
   14     | TK_PR_STRING

   15 decl_variavel: tipo ':' TK_IDENTIFICADOR ';'

   16 decl_vetor: tipo ':' TK_IDENTIFICADOR '[' TK_LIT_INT ']' ';'

   17 decl_parametro: tipo ':' TK_IDENTIFICADOR
   18               | tipo ':' TK_IDENTIFICADOR ',' decl_parametro

   19 decl_funcao: cabecalho decl_locais bloco
   20            | cabecalho bloco

   21 cabecalho: tipo ':' TK_IDENTIFICADOR '(' parametros ')'
   22          | tipo ':' TK_IDENTIFICADOR '(' ')'

   23 parametros: decl_parametro parametros
   24           | decl_parametro

   25 decl_locais: decl_variavel decl_locais
   26            | decl_variavel

   27 bloco: '{' comandos '}'
   28      | '{' '}'

   29 comandos: comando_simples ';' comandos
   30         | fluxo_controle comandos
   31         | comando_simples ';'
   32         | fluxo_controle
   33         | bloco comandos
   34         | bloco
   35         | ';'

   36 comando: comando_simples
   37        | fluxo_controle

   38 comando_simples: atribuicao
   39                | entrada
   40                | saida
   41                | retorno
   42                | chamada_funcao

   43 atribuicao: TK_IDENTIFICADOR '=' expressao
   44           | TK_IDENTIFICADOR '=' TK_LIT_STRING
   45           | TK_IDENTIFICADOR '=' TK_LIT_CHAR
   46           | TK_IDENTIFICADOR '[' expressao ']' '=' expressao
   47           | TK_IDENTIFICADOR '[' expressao ']' '=' TK_LIT_STRING
   48           | TK_IDENTIFICADOR '[' expressao ']' '=' TK_LIT_CHAR

   49 entrada: TK_PR_INPUT TK_IDENTIFICADOR

   50 saida: TK_PR_OUTPUT lista_elementos

   51 lista_elementos: TK_LIT_STRING
   52                | TK_LIT_STRING ',' lista_elementos
   53                | expressao
   54                | expressao ',' lista_elementos

   55 retorno: TK_PR_RETURN expressao

   56 chamada_funcao: TK_IDENTIFICADOR '(' lista_argumentos ')'

   57 expressao_aritmetica: expressao '+' expressao
   58                     | expressao '-' expressao
   59                     | expressao '*' expressao
   60                     | expressao '/' expressao

   61 expressao_logica: expressao TK_OC_LE expressao
   62                 | expressao TK_OC_GE expressao
   63                 | expressao '<' expressao
   64                 | expressao TK_OC_EQ expressao
   65                 | expressao TK_OC_NE expressao
   66                 | expressao TK_OC_AND expressao
   67                 | expressao TK_OC_OR expressao

   68 expressao: expressao_aritmetica
   69          | expressao_logica
   70          | '(' expressao ')'
   71          | TK_IDENTIFICADOR '[' expressao ']'
   72          | TK_IDENTIFICADOR
   73          | TK_LIT_INT
   74          | '-' TK_LIT_INT
   75          | TK_LIT_TRUE
   76          | TK_LIT_FALSE
   77          | TK_LIT_FLOAT
   78          | '-' TK_LIT_FLOAT
   79          | chamada_funcao

   80 lista_argumentos: TK_LIT_CHAR
   81                 | TK_LIT_CHAR ',' lista_argumentos
   82                 | TK_LIT_STRING
   83                 | TK_LIT_STRING ',' lista_argumentos
   84                 | expressao
   85                 | expressao ',' lista_argumentos
   86                 | /* empty */

   87 fluxo_controle: TK_PR_IF '(' expressao_logica ')' TK_PR_THEN then_else
   88               | TK_PR_IF '(' expressao_logica ')' TK_PR_THEN then TK_PR_ELSE then_else
   89               | TK_PR_WHILE '(' expressao_logica ')' TK_PR_DO comandos_while_do ';'
   90               | TK_PR_DO comandos_while_do TK_PR_WHILE '(' expressao_logica ')' ';'

   91 then: comando
   92     | bloco

   93 then_else: comando ';'
   94          | bloco ';'

   95 comandos_while_do: comando
   96                  | bloco


